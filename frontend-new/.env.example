# Example environment settings - copy to .env.local to use

# Local development settings
# Set to true to enable mock mode and avoid real API calls
REACT_APP_STRICT_MOCK_MODE=true
REACT_APP_MOCK_MODE=true

# API settings
# URL for the backend API
REACT_APP_API_URL=http://localhost:5001

# Set to true to avoid health check error logs
REACT_APP_DISABLE_HEALTH_CHECK=true 